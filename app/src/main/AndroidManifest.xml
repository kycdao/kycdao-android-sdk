<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kycdao.android.sdk">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.kycdao.android.sdk.ExampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="false">

        <activity
            android:name="com.kycdao.android.sdk.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.kycdao.android.sdk.ui.SdkActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:exported="false" />

        <activity
            android:name="com.kycdao.android.sdk.ui.ProgressActivity"
            android:label="Demo"
            android:exported="false" />

    </application>

</manifest>